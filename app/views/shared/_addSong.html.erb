<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>

<b>Ma gasesti la shared/_addSong</b>

 <div class="form-group">
    <%= f.label :username %><br />
    <%= f.text_field :username, :autofocus => true, :placeholder => "Enter username", :class => "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :password %><br />
    <%= f.password_field :password, :class => "form-control" %>
  </div>
	
  
  <% if devise_mapping.rememberable? -%>
  	<div class="form-group">
    	<%= f.check_box :remember_me %> 
    	<%= f.label :remember_me %>
    </div>
  <% end -%>
  
   <%= f.submit "Sign in" , :class => "btn btn-default"%>
<% end %>

<%= link_to "Forgot your password?", new_password_path(resource_name) ,:class => "btn btn-info"%><br />

